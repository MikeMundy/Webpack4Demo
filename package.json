{
  "name": "Webpack4Demo",
  "version": "1.0.0",
  "description": "See https://webpack.js.org/guides/getting-started/\r npm init -y\r npm install --save-dev webpack",
  "scripts": {
    "build": "webpack --config webpack/webpack.dev.config.js",
    "start": "webpack-dev-server --open --config webpack/webpack.dev.config.js",
    "test": "jest",
    "test_feathers": "npm run eslint_feathers && npm run mocha",
    "eslint_feathers": "eslint src/. test/. --config .eslintrc.json",
    "dev_feathers": "nodemon src/",
    "start_feathers": "node src/",
    "mocha_feathers": "mocha test/ --recursive --exit"
  },
  "main": "index.js",
  "main_feathers": "src",
  "keywords": [],
  "author": {
    "name": "Michael Mundy",
    "email": "seraphrim@hotmail.com"
  },
  "contributors": [],
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^9.0.0",
    "npm": ">= 3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.105",
    "@types/react": "^16.4.7",
    "@types/react-redux": "6.0.2",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux-actions": "2.3.0",
    "eslint": "^5.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "awesome-typescript-loader": "^5.0.0-1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "csv-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^23.4.2",
    "mini-css-extract-plugin": "^0.2.0",
    "node-sass": "^4.9.2",
    "react-hot-loader": "^4.3.1",
    "redux": "^4.0.0",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "ts-jest": "^23.1.2",
    "ts-loader": "^4.0.1",
    "typescript": "^2.9.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.7",
    "@feathersjs/authentication-jwt": "^2.0.1",
    "@feathersjs/authentication-local": "^1.2.1",
    "@feathersjs/configuration": "^2.0.0",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.3",
    "@feathersjs/feathers": "^3.1.7",
    "@feathersjs/socketio": "^3.2.2",
    "@types/react-dom": "^16.0.6",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "feathers-nedb": "^3.1.0",
    "helmet": "^3.13.0",
    "lodash": "^4.17.5",
    "nedb": "^1.8.0",
    "react": "^16.2.0",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
